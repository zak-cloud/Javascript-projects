		var sliderAmount = document.getElementById('slider-amount');
var sliderPeriod = document.getElementById('slider-period');
noUiSlider.create(sliderAmount, {
    start: 5,
    step: 1,
    connect: "lower",
    range: {
        'min': 0,
        'max': amounts.length-1
    }

});

 
noUiSlider.create(sliderPeriod, {
    start: 5,
    step: 1,
    connect: "lower",
    range: {
        'min': 0,
        'max': periods.length-1
    }

});
sliderAmount.noUiSlider.on('update', function (values, handle) {
   //getting the slider value
   var amount_index = parseInt(values[handle]);
//    console.log(amount_index + '   ' + amounts[amount_index]);

   //setting the value to the slider tooltip
   $("#slider-amount").find(".noUi-handle[data-handle="+handle+"] .noUi-tooltip").text(amounts[amount_index]);
   //setting value to the div
   $(".js-amount").html(amounts[amount_index]);
});
sliderPeriod.noUiSlider.on('update', function (values, handle) {
   //getting the slider value
   var period_index = parseInt(values[handle]);
   
 //    console.log(amount_index + '   ' + amounts[amount_index]);

   //setting the value to the slider tooltip
   $("#slider-period").find(".noUi-handle[data-handle="+handle+"] .noUi-tooltip").text(periods[period_index]);
   //setting value to the div
   $(".js-period").html(periods[period_index]);

   var currentDate = new Date();
var tomorrow = new Date();
tomorrow.setDate(today.getDate() + 1);
var day = tomorrow.getDate();
var month = tomorrow.getMonth() + 1;
var year = tomorrow.getFullYear();
var returnDate = year + "-" + month + "-" + day;


$(".date").html(tomorrow);
$(".return-date").html(returnDate);

});

